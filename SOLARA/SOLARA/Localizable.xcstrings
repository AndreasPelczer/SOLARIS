    "shop.item.stone.subtitle" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Dieser Stein heilt nichts.\nEr harmonisiert nichts.\nEr ist nicht zertifiziert.\nEr ist rein dekorative Überzeugung." } },
        "en" : { "stringUnit" : { "state" : "translated", "value" : "This stone heals nothing.\nIt harmonizes nothing.\nIt is not certified by anyone.\nIt is decorative conviction." } },
        "es" : { "stringUnit" : { "state" : "translated", "value" : "Esta piedra no sana nada.\nNo armoniza nada.\nNo está certificada por nadie.\nEs pura convicción decorativa." } },
        "pt-BR" : { "stringUnit" : { "state" : "translated", "value" : "Essa pedra não cura nada.\nNão harmoniza nada.\nNão é certificada por ninguém.\nÉ pura convicção decorativa." } }
      }
    },
    "shop.item.mercurykit.title" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Merkur-Rückläufig-Kit" } },
