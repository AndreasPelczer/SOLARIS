    "shop.item.tea.subtitle" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Ein digitaler Tee für dein inneres Gleichgewicht.\nEr wärmt nicht. Er duftet nicht.\nEr existiert. Und das ist schon erstaunlich genug." } },
        "en" : { "stringUnit" : { "state" : "translated", "value" : "A digital tea for your inner balance.\nIt does not warm you. It does not smell.\nIts existence is already impressive." } },
        "es" : { "stringUnit" : { "state" : "translated", "value" : "Un té digital para tu equilibrio interior.\nNo calienta. No huele.\nSu existencia ya es bastante impresionante." } },
        "pt-BR" : { "stringUnit" : { "state" : "translated", "value" : "Um chá digital para seu equilíbrio interior.\nNão esquenta. Não tem aroma.\nO simples fato de existir já é impressionante." } }
      }
    },
    "shop.item.stone.title" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Virtueller Heilstein" } },
