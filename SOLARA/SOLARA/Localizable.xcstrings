    "shop.item.mercurykit.subtitle" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Falls heute alles schief läuft, liegt es vermutlich nicht an Merkur.\nDieses Kit hilft ebenfalls nicht.\nAber es gibt dir etwas, dem du die Schuld geben kannst." } },
        "en" : { "stringUnit" : { "state" : "translated", "value" : "If everything goes wrong today, it is probably not Mercury's fault.\nThis kit will not help either.\nBut it gives you something to blame." } },
        "es" : { "stringUnit" : { "state" : "translated", "value" : "Si hoy todo sale mal, probablemente no sea culpa de Mercurio.\nEste kit tampoco ayuda.\nPero al menos tienes algo a quien culpar." } },
        "pt-BR" : { "stringUnit" : { "state" : "translated", "value" : "Se hoje tudo deu errado, provavelmente não é culpa de Mercúrio.\nEsse kit também não ajuda.\nMas pelo menos você tem algo para culpar." } }
