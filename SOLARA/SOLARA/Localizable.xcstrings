    "shop.intro" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Hier findest du Dinge, die nichts tun.\nWirklich. Gar nichts.\nSie erhöhen nicht deine Schwingung, sie reinigen keine Aura\nund sie beschleunigen definitiv keine Manifestation.\nWenn du SOLARA unterstützen möchtest, kannst du das hier tun.\nWenn nicht, funktioniert alles weiterhin exakt genauso." } },
        "en" : { "stringUnit" : { "state" : "translated", "value" : "Here you will find objects of profound uselessness.\nThey raise no vibration, cleanse no aura,\nand accelerate absolutely nothing.\nIf you wish to support SOLARA, you may do so here.\nIf not, everything continues exactly as before." } },
        "es" : { "stringUnit" : { "state" : "translated", "value" : "Aquí encontrarás objetos de inutilidad sublime.\nNo elevan tu vibración, no limpian tu aura\ny no aceleran absolutamente nada.\nSi quieres apoyar SOLARA, puedes hacerlo aquí.\nSi no, todo seguirá exactamente igual." } },
        "pt-BR" : { "stringUnit" : { "state" : "translated", "value" : "Aqui você encontra objetos de inutilidade elevada.\nNão aumentam sua vibração, não limpam sua aura\ne definitivamente não aceleram nada.\nSe quiser apoiar o SOLARA, pode fazer isso aqui.\nSe não, tudo continua exatamente igual." } }
      }
    },
    "shop.disclaimer" : {
      "localizations" : {
        "de" : { "stringUnit" : { "state" : "translated", "value" : "Alle Artikel sind freiwillige In-App-Käufe ohne funktionalen Nutzen.\nSie verändern nichts – außer vielleicht dein Gewissen." } },
        "en" : { "stringUnit" : { "state" : "translated", "value" : "All items are optional in-app purchases with no functional benefit.\nThey change nothing — except possibly your conscience." } },
        "es" : { "stringUnit" : { "state" : "translated", "value" : "Todos los artículos son compras opcionales sin ningún beneficio funcional.\nNo cambian nada… excepto quizás tu conciencia." } },
        "pt-BR" : { "stringUnit" : { "state" : "translated", "value" : "Todos os itens são compras opcionais sem benefício funcional.\nNão mudam nada — talvez só a sua consciência." } }
      }
    },
